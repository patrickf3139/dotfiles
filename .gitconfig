[user]
  email = patrickf3139@gmail.com
  name = Patrick Fong
[push]
  default = simple
[pull]
  ff = only
[core]
  editor = nvim
  excludesfile = ~/.gitignore_global
  pager = delta
[interactive]
  diffFilter = delta --color-only
[delta]
  features = line-numbers decorations
  whitespace-error-style = 22 reverse
  hunk-header = omit
[alias]
  aliases = config --get-regexp ^alias\\.
  amend = commit --all --amend --no-edit
  br = branch
  co = checkout
  d = diff
  dc = diff --cached
  fp = push --force
  lg = log --graph --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(cyan)%aD%C(reset) %C(green)(%ar)%C(reset)%C(yellow)%d%C(reset)%n          %C(normal)%s%C(reset)   %C(dim normal)[%an]%C(reset)'
  rb = push --set-upstream origin HEAD
  rh = reset --hard
  st = status --short
  undo = "reset --hard HEAD^"
[diff]
  algorithm = patience
[fetch]
  prune = true
